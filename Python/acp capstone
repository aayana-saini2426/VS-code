CREATE TABLE IF NOT EXISTS BAKERY_EMPLOYEES(
  EMP_ID TEXT,
  EMP_NAME TEXT,
  POSITION TEXT,
  PRODUCTS_SOLD INTEGER,
  SALARY REAL,
  EXPERIENCE INTEGER,
  BRANCH TEXT,
  CONTACT_NO TEXT
);

INSERT INTO BAKERY_EMPLOYEES(EMP_ID, EMP_NAME, POSITION, PRODUCTS_SOLD, SALARY, EXPERIENCE, BRANCH, CONTACT_NO) VALUES
('E01','Harsh Mehta','Manager',320,65000,7,'Main Branch','9876543210'),
