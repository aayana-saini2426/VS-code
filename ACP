CREATE TABLE IF NOT EXISTS CUSTOMER(
  CUSTOMER_ID TEXT,
  CUSTOMER_NAME TEXT,
  CITY TEXT,
  GRADE INTEGER
);

INSERT INTO CUSTOMER(CUSTOMER_ID, CUSTOMER_NAME, CITY, GRADE) VALUES
('C101','RAMESH','New York',120),
('C102','SURESH','London',90),
('C103','RAHUL','New York',80),
('C104','NITA','Paris',150),
('C105','GEETA','New York',110),
('C106','MOHAN','Tokyo',70);

SELECT *
FROM CUSTOMER
WHERE CITY = 'New York' OR GRADE > 100;

